  <link href="images/pixel-creator-fav-small.png" rel="shortcut icon" type="image/x-icon">
  <link href="images/pixel-creator-fav-large.png" rel="apple-touch-icon">
  <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.4.7/angular.js"></script>
