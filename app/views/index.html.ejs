<body class="body2" ng-app="app">
  <div class="header w-clearfix"><a href="#" class="brand-2 w-inline-block w-clearfix"><img src="images/pixel-creator-logo-dark.png" width="40" class="signin-logo"><div class="logo-text">Pixel<span class="text-span-9">Creator</span></div></a>
    <div class="sigin-info-block w-clearfix">
      <div class="create-account"> <a href="/create-an-account" class="link blue">Forgot Password</a></div>
      <div class="create-account line-seperate">New to PixelCreator? <a href="/create-an-account" class="link blue">Create an Account</a></div>
    </div>
  </div>

  <div class="info-pane">
    <div class="sigin-form" >
      <div class="sign-in-form w-form" data-ng-controller="AuthenticationController">
        <h2 class="heading">Welcome Back</h2>
        <p class="login-text">Login to your Pixel Projects</p>

        <form  name="email-form" data-name="Email Form" data-ng-submit="signin()" ><a href="/auth/twitter" class="login-with-twitter w-inline-block"><img src="images/twitter.svg" width="23" class="twitter-logo-sigin"><div class="twitter-sigin">Sign in with Twitter</div></a>

          <a href="/auth/facebook" class="login-with-twitter facebook-signin w-inline-block"><img src="images/facebook2.svg" width="23" class="twitter-logo-sigin"><div class="twitter-sigin">Sign in with Facebook</div></a>

          <div class="or">- OR -</div><input type="text" class="sign-form w-input" maxlength="256" name="Username-or-Email" data-name="Username or Email" placeholder="Enter Your Username or Email" id="Username-or-Email" data-ng-model="credentials.username" >
          <input type="password" class="sign-form w-input" maxlength="256" name="Password" data-name="Password" placeholder="Enter your Password" id="Password" data-ng-model="credentials.password"  required="">
          <button type="submit" class="signin-button w-inline-block">
           Sign in
          </button>
        </form>

        <div class="w-form-done">
          <div>Thank you! Your submission has been received!</div>
        </div>
        <div class="w-form-fail">
          <div>Oops! Wrong username or Password!.</div>
        </div>
      </div>
    </div>
  </div>

<script src="https://code.jquery.com/jquery-3.3.1.min.js" type="text/javascript" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
