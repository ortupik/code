<body class="body2" ng-app="app">
  <div class="header w-clearfix"><a href="#" class="brand-2 w-inline-block w-clearfix"><img src="images/pixel-creator-logo-dark.png" width="40" class="signin-logo"><div class="logo-text">Pixel<span class="text-span-9">Creator</span></div></a>
    <div class="sigin-info-block w-clearfix">
      <div class="create-account"> <a href="create-an-account.html" class="link blue w--current">Forgot Password</a></div>
      <div class="create-account line-seperate">Already a PixelCreator member? <a href="index.html" class="link blue">Sign Up</a></div>
    </div>
  </div>
  <div class="info-pane">
    <div class="welcome-back-pane create-account-pain">
      <div class="welcome-text"></div>
    </div>
    <div class="create-an-account">
      <div class="sign-in-form w-form" data-ng-controller="AuthenticationController">
        <h2 class="heading">Create an Account</h2>
        <p class="login-text">Its easy to get started with your first Pixel Project</p>

        <form id="email-form" name="email-form" data-name="Email Form" class="form-5" data-ng-submit="signup()">

          <a href="/auth/twitter" class="login-with-twitter w-inline-block"><img src="images/twitter.svg" width="23" class="twitter-logo-sigin"><div class="twitter-sigin">Create an Account with Twitter</div></a>

          <a href="/auth/facebook" class="login-with-twitter facebook-signin w-inline-block"><img src="images/facebook2.svg" width="23" class="twitter-logo-sigin"><div class="twitter-sigin">Create an Account with Facebook</div></a>

          <div class="or">- OR -</div>

          <input type="text" class="sign-form w-input" maxlength="256" name="Name" data-name="Name" placeholder="Enter Your Name" id="Name" data-ng-model="credentials.name" >
          <input type="text" class="sign-form w-input" maxlength="256" name="Username" data-name="Username" placeholder="Pick a Username" id="Username" data-ng-model="credentials.username" >
          <input type="text" class="sign-form w-input" maxlength="256" name="Email-create-new-account" data-name="Email-create-new-account" placeholder="Enter your Email" id="Email-create-new-account" data-ng-model="credentials.email" >
          <input type="password" class="sign-form w-input" maxlength="256" name="Password" data-name="Password" placeholder="Create a Password" id="Password" required="" data-ng-model="credentials.password" >
          <input type="password" class="sign-form w-input" maxlength="256" name="Password-2" data-name="Password 2" placeholder="Verify Password" id="Password-2" required="" >
          <input type="submit" value="Sign In" data-wait="Please wait..." class="signin-button w-button">

        </form>

        <div class="w-form-done">
          <div>Thank you! Your submission has been received!</div>
        </div>
        <div class="w-form-fail">
          <div>Oops! Something went wrong while submitting the form.</div>
        </div>
        <div class="w-form-fail pass_failure">
          <div>Oops! Passwords don't match!</div>
        </div>
      </div>
    </div>
  </div>
  <script src="https://code.jquery.com/jquery-3.3.1.min.js" type="text/javascript" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
